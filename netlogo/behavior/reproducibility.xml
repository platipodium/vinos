    <enumeratedValueSet variable="show-ports?">
      <value value="false"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="wage">
      <value value="50"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="monthly-chooser">
      <value value="&quot;Fleet effort h&quot;"/>
    </enumeratedValueSet>
  </experiment>
</experiments>
