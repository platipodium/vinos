<!DOCTYPE experiments SYSTEM "behaviorspace.dtd">
<experiments>
  <experiment name="vary-wage" repetitions="1" runMetricsEveryStep="true">
    <setup>setup</setup>
    <go>go</go>
    <timeLimit steps="1000"/>
    <metric>["annual-average fleet-monthly-hours"]</metric>
    <enumeratedValueSet variable="time-offset">
      <value value="-24"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="output-frequency">
      <value value="&quot;monthly&quot;"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="adaptation">
      <value value="0.851"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="memory-size">
      <value value="20"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="diesel-price">
      <value value="80"/>
    </enumeratedValueSet>
    <steppedValueSet variable="wage" first="20" step="10" last="120"/>
  </experiment>
</experiments>
