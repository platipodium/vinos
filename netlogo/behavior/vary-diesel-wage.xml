<!DOCTYPE experiments SYSTEM "behaviorspace.dtd">
<experiments>
  <experiment name="vary-diesel-wage" repetitions="1" runMetricsEveryStep="true">
    <setup>setup</setup>
    <go>go</go>
    <timeLimit steps="1000"/>
    <metric>fleet-monthly-revenue</metric>
    <metric>fleet-monthly-landing</metric>
    <metric>fleet-monthly-hours</metric>
    <enumeratedValueSet variable="time-offset">
      <value value="-24"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="output-frequency">
      <value value="&quot;monthly&quot;"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="adaptation">
      <value value="0.899"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="memory-size">
      <value value="20"/>
    </enumeratedValueSet>
    <steppedValueSet variable="diesel-price" first="20" step="15" last="110"/>
    <steppedValueSet variable="wage" first="50" step="25" last="125"/>
  </experiment>
</experiments>
