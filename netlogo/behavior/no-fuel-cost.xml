<!DOCTYPE experiments SYSTEM "behaviorspace.dtd">
<experiments>
  <experiment name="no-fuel-cost" repetitions="1" runMetricsEveryStep="true">
    <setup>setup</setup>
    <go>go</go>
    <timeLimit steps="1000"/>
    <metric>fleet-monthly-revenue</metric>
    <metric>fleet-monthly-landing</metric>
    <metric>fleet-monthly-hours</metric>
    <enumeratedValueSet variable="time-offset">
      <value value="-24"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="one?">
      <value value="false"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="output-frequency">
      <value value="&quot;weekly&quot;"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="adaptation">
      <value value="0.809"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="memory-size">
      <value value="20"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="diesel-price">
      <value value="0"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="wage">
      <value value="50"/>
    </enumeratedValueSet>
  </experiment>
</experiments>
